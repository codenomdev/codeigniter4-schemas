{
    "name": "codenom/schemas",
    "description": "Database schema management, for CodeIgniter 4",
    "keywords": [
        "codeigniter",
        "codeigniter4",
        "schemas",
        "database",
        "mapping",
        "structure"
    ],
    "homepage": "https://github.com/codenom/codeigniter4-schemas",
    "license": "MIT",
    "authors": [
        {
            "name": "Database Schemas for CodeIgniter 4",
            "email": "dev@codenom.com",
            "homepage": "https://codenom.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/codeigniter4/CodeIgniter4"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "mockery/mockery": "^1.0",
        "codeigniter4/codeigniter4": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Codenom\\Schemas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    }
}